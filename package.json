{
  "name": "yieldmachine",
  "amdName": "yieldmachine",
  "version": "0.5.0",
  "description": "Declarative state machines using generator functions",
  "source": "src/index.ts",
  "main": "dist/yieldmachine.js",
  "module": "dist/yieldmachine.module.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "browser": "./dist/yieldmachine.module.js",
      "import": "./dist/yieldmachine.module.js",
      "require": "./dist/yieldmachine.js"
    }
  },
  "targets": {
    "main": {
      "optimize": true
    },
    "module": {
      "optimize": true
    }
  },
  "scripts": {
    "prepack": "jest && npm run build && node tests/node.js",
    "dev": "parcel watch",
    "build": "parcel build",
    "test": "jest --watch"
  },
  "author": "Patrick Smith",
  "license": "MIT",
  "devDependencies": {
    "@parcel/packager-ts": "^2.0.0",
    "@parcel/transformer-typescript-types": "^2.0.0",
    "@testing-library/dom": "^8.12.0",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/user-event": "^14.0.4",
    "@types/jest": "^27.4.1",
    "jest": "^27.5.1",
    "parcel": "^2.0.0",
    "ts-jest": "^27.0.5",
    "typescript": "^4.6.3"
  }
}
